@page "/logout"
@inject IJSRuntime JSRuntime
@inject HttpClient HttpClient
@using eCommerce.ComputerParts.Shop.Web.Admin.JavaScript
@inherits eCommerce.ComputerParts.Shop.Web.Admin.Helpers.BlazorComponent

@code {

    protected override async Task OnInitializedAsync()
    {
        await HttpClient.PostAsync("User/Logout", null);
        await new Route(JSRuntime).RouteOutside("/Identity/Account/Login");
    }

}
